{
  "author": "Bling-io",
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/format": "^19.5.0",
    "@commitlint/prompt-cli": "^19.5.0",
    "@commitlint/read": "^19.5.0",
    "@packages/typescript": "workspace:^",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "syncpack": "^13.0.0",
    "turbo": "^2.3.0",
    "typescript": "catalog:"
  },
  "engineStrict": true,
  "engines": {
    "node": "^22"
  },
  "keywords": [],
  "license": "ISC",
  "lint-staged": {
    "*": "pnpm biome check --write --unsafe --no-errors-on-unmatched --verbose"
  },
  "name": "finceio",
  "packageManager": "pnpm@9.13.2+sha512.88c9c3864450350e65a33587ab801acf946d7c814ed1134da4a924f6df5a2120fd36b46aab68f7cd1d413149112d53c7db3a4136624cfd00ff1846a0c6cef48a",
  "private": true,
  "scripts": {
    "b": "pnpm --filter=@backend/*",
    "b:client": "pnpm --filter=@backend/client",
    "b:client:build": "pnpm turbo run build --filter=@backend/client",
    "b:client:dev": "pnpm turbo run dev --filter=@backend/client",
    "b:client:start": "pnpm turbo run start --filter=@backend/client",
    "build": "turbo run build",
    "check-types": "pnpm tsc --noEmit",
    "dev": "turbo run dev",
    "f": "pnpm --filter=@frontend/*",
    "f:web": "pnpm --filter=@frontend/web",
    "f:web:build": "pnpm turbo run build --filter=@frontend/web",
    "f:web:dev": "pnpm turbo run dev --filter=@frontend/web -- --turbo",
    "lint": "turbo run lint",
    "p": "pnpm --filter=@packages/*",
    "p:magic": "pnpm --filter=@packages/magic",
    "p:plate": "pnpm --filter=@packages/plate",
    "p:shadcn": "pnpm --filter=@packages/shadcn"
  },
  "version": "1.0.0"
}
